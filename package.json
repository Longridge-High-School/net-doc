{
  "name": "net-doc",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "npm-run-all -p dev:*",
    "dev:remix": "remix vite:dev --manual",
    "dev:worker": "env-cmd -f .env node ./worker/worker.mjs",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "docker": "prisma migrate deploy && prisma generate && prisma db seed && npm run start",
    "prettier": "prettier app --check --end-of-line auto",
    "test": "env-cmd -f .test.env vitest",
    "test:ci": "vitest",
    "test:db:create": "env-cmd -f .test.env npx prisma migrate dev"
  },
  "dependencies": {
    "@arcath/utils": "^3.15.0",
    "@epic-web/totp": "^1.1.2",
    "@prisma/client": "^5.17.0",
    "@remix-run/node": "^2.10.3",
    "@remix-run/react": "^2.10.3",
    "@remix-run/serve": "^2.10.3",
    "@tanstack/react-query": "^5.51.11",
    "adm-zip": "^0.5.14",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.10.4",
    "cancant": "^1.1.0",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.3.8",
    "is-ip": "^5.0.1",
    "isbot": "^5.1.13",
    "mdx-bundler": "^10.0.2",
    "mkdirp": "^3.0.1",
    "node-cron": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "qrcode": "^1.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use": "^17.5.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@remix-run/dev": "^2.10.3",
    "@types/bcrypt": "^5.0.2",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "autoprefixer": "^10.4.19",
    "env-cmd": "^10.1.0",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.5.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}

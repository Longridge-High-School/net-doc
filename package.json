{
  "name": "net-doc",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "npm-run-all -p dev:*",
    "dev:remix": "remix vite:dev --manual",
    "dev:worker": "env-cmd -f .env node ./worker/worker.mjs",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start:remix": "remix-serve ./build/server/index.js",
    "start:worker": "node ./worker/worker.mjs",
    "typecheck": "tsc",
    "docker": "prisma migrate deploy && prisma generate --sql && prisma db seed && npm-run-all -p start:*",
    "prettier": "prettier app --check --end-of-line auto",
    "test": "env-cmd -f .test.env vitest",
    "test:coverage": "env-cmd -f .test.env vitest run --coverage",
    "test:ci": "vitest run --coverage",
    "test:db:create": "env-cmd -f .test.env npx prisma migrate dev"
  },
  "dependencies": {
    "@arcath/utils": "^3.15.0",
    "@epic-web/totp": "^2.1.1",
    "@prisma/client": "^6.3.0",
    "@remix-run/node": "^2.15.2",
    "@remix-run/react": "^2.15.2",
    "@remix-run/serve": "^2.15.2",
    "@tanstack/react-query": "^5.65.1",
    "adm-zip": "^0.5.16",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.38.0",
    "cancant": "^1.1.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.0.6",
    "is-ip": "^5.0.1",
    "isbot": "^5.1.21",
    "mdx-bundler": "^10.0.3",
    "mkdirp": "^3.0.1",
    "node-cron": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "nprogress": "^0.2.0",
    "papaparse": "^5.5.2",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hotkeys-hook": "^4.6.1",
    "react-use": "^17.6.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@faker-js/faker": "^9.4.0",
    "@remix-run/dev": "^2.15.2",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/nprogress": "^0.2.3",
    "@types/papaparse": "^5.3.15",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@vitest/coverage-istanbul": "^3.0.4",
    "@vitest/eslint-plugin": "^1.1.25",
    "autoprefixer": "^10.4.20",
    "env-cmd": "^10.1.0",
    "eslint": "^9.19.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prisma": "^6.3.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
